{
  "name": "@minni-im/tape-recorder",
  "version": "5.0.0-beta.5",
  "description": "CouchDB nodejs lightweight ORM",
  "type": "module",
  "main": "./index.js",
  "exports": "./index.js",
  "types": "index.d.ts",
  "files": [
    "bin",
    "scripts",
    "index.js",
    "index.d.ts",
    "package.json",
    "readme.md"
  ],
  "bin": {
    "tape-recorder": "bin/cli.js"
  },
  "scripts": {
    "test": "uvu",
    "test:watch": "watchlist index.js test.js -- npm test",
    "test:coverage": "c8 --include=index.js npm test && c8 report --reporter=html",
    "postinstall": "node scripts/postinstall.cjs"
  },
  "keywords": [
    "couchdb",
    "orm",
    "node",
    "database",
    "db",
    "nosql"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minni-im/tape-recorder.git"
  },
  "author": {
    "name": "Benoit Charbonnier",
    "email": "benoit.charbonnier@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/minni-im/tape-recorder/issues"
  },
  "homepage": "https://github.com/minni-im/tape-recorder#readme",
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "c8": "^7.11.2",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "node-fetch": "^3.2.10",
    "prettier": "^2.7.1",
    "uvu": "^0.5.6",
    "watchlist": "^0.3.1"
  },
  "node": {
    "engine": ">= 16"
  },
  "sideEffects": false
}
